<%# The below code creates an array of 10 random names %>
<%= names = Array.new(10) { Faker::Name.unique.name  } %>
<% numbers = Array.new(10) { rand(1..10) } %>

<div class="col-md-4 offset-md-4">
    <ul class="list-group">
      <% names.each_with_index do |name, i| %>
        <li class="list-group-item d-flex justify-content-between align-items-center"><%= name %>
        <span class="badge text-bg-primary rounded-pill"><%= numbers[i] %></span></li>
      <% end %>
    </ul>
</div>
